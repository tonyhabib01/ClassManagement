{% extends '@SonataAdmin/CRUD/base_show_field.html.twig' %}

{% block field %}
    <table>
        <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Grade</th>
        </tr>
        </thead>
        <tbody>
        {% for i in value %}
            <tr>
                <td>{{ i.firstName }}</td>
                <td>{{ i.lastName }}</td>
                <td>{{ i.gpa }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


{% endblock %}